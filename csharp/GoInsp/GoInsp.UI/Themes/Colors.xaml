<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Colors-->
    <Color x:Key="ColorGray1">#ffffff</Color>
    <Color x:Key="ColorGray2">#f4f4f4</Color>
    <Color x:Key="ColorGray3">#ebebeb</Color>
    <Color x:Key="ColorGray4">#e0e0e0</Color>
    <Color x:Key="ColorGray5">#7a7a7a</Color>
    <!--/Colors-->
    
    <!--Brushes-->
    <SolidColorBrush x:Key="BrushGray1" Color="{StaticResource ColorGray1}"/>
    <SolidColorBrush x:Key="BrushGray2" Color="{StaticResource ColorGray2}"/>
    <SolidColorBrush x:Key="BrushGray3" Color="{StaticResource ColorGray3}"/>
    <SolidColorBrush x:Key="BrushGray4" Color="{StaticResource ColorGray4}"/>
    <SolidColorBrush x:Key="BrushGray5" Color="{StaticResource ColorGray5}"/>
    <!--/Brushes-->
    
</ResourceDictionary>